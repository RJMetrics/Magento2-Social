<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <head>
        <title>Connect your store to Facebook</title>
        <css src="Magento_Social::css/main.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Social\Block\Adminhtml\Facebook\Manage" name="social.facebook.manage"
                   template="Magento_Social::social_facebook_manage.phtml"/>
        </referenceContainer>
    </body>
</page>
